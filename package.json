{
  "name": "gamedevjs-jam-2021",
  "version": "1.0.0",
  "description": "A game for the Gamedev.js 2021 Jam",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --color --mode=development",
    "build": "npm run build:contract && webpack --mode=production",
    "build:contract": "node contract/compile.js",
    "build:contract:debug": "node contract/compile.js --debug",
    "dev:deploy:contract": "near dev-deploy",
    "deploy:contract": "near deploy",
    "deploy": "npm run build && npm run deploy:contract",
    "prestart": "npm run build:contract:debug && npm run dev:deploy:contract",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "near-cli": "^1.6.0",
    "shelljs": "^0.8.4",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "near-api-js": "^0.39.0",
    "phaser": "^3.54.0",
    "phaser3-rex-plugins": "^1.1.45"
  }
}
